{<% var ngVer = "1.3.*" %>
  "name": "<%= _.slugify(_.humanize(appname)) %>",
  "version": "0.0.0",
  "dependencies": {
    "angular": ">=<%= ngVer %>",
    "json3": "~3.3.2",
    "es5-shim": "~4.0.3",<% if(filters.bootstrap) { %><% if (filters.sass) { %>
    "bootstrap-sass-official": "~3.3.0",<% } %>
    "bootstrap": "~3.3.0",<% } %>
    "angular-resource": ">=<%= ngVer %>",
    "angular-cookies": ">=<%= ngVer %>",
    "angular-sanitize": ">=<%= ngVer %>",<% if(filters.ngroute) { %>
    "angular-route": ">=<%= ngVer %>",<% } %><% if(filters.uibootstrap) { %>
    "angular-bootstrap": "~0.11.2",<% } %>
    "font-awesome": ">=4.2.0",
    "lodash": "~2.4.1"<% if(filters.socketio) { %>,
    "angular-socket-io": "~0.6.0"<% } %><% if(filters.uirouter) { %>,
    "angular-ui-router": "~0.2.11"<% } %>
  },
  "devDependencies": {
    "angular-mocks": ">=<%= ngVer %>",
    "angular-scenario": ">=<%= ngVer %>"
  }
}
